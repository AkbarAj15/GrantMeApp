<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".penyedia_FragHome"
    android:background="@color/white">

    <View
        android:id="@+id/rectangle_1"
        android:layout_width="335dp"
        android:layout_height="160dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/kotakbiru_home" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="58dp"
        android:layout_height="35dp"
        android:layout_alignStart="@+id/rectangle_1"
        android:layout_alignTop="@+id/rectangle_1"
        android:layout_alignBottom="@+id/rectangle_1"
        android:layout_marginStart="32dp"
        android:layout_marginTop="22dp"
        android:layout_marginBottom="103dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Halo,"
        android:textColor="@color/white"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="255dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rectangle_1"
        android:layout_alignParentStart="true"
        android:layout_marginStart="23dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Daftar Beasiswa Anda"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="318dp"
        android:layout_height="25dp"
        android:layout_alignStart="@+id/rectangle_1"
        android:layout_alignEnd="@+id/rectangle_1"
        android:layout_alignBottom="@+id/rectangle_1"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="21dp"
        android:layout_marginBottom="79dp"
        android:fontFamily="@font/poppins"
        android:text="Ayo bantu orang-orang dengan beasiswa!"
        android:textColor="@color/white"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="97dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/rectangle_1"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="514dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Lihat semua"
        android:textColor="#6E85B7"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/userPenyedia"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_alignTop="@+id/rectangle_1"
        android:layout_alignBottom="@+id/rectangle_1"
        android:layout_marginStart="8dp"
        android:layout_marginTop="22dp"
        android:layout_marginBottom="106dp"
        android:layout_toEndOf="@+id/textView5"
        android:fontFamily="@font/poppins_bold"
        android:hint="nama pengguna"
        android:textColor="@color/white"
        android:textSize="20sp" />

    <EditText
        android:id="@+id/notelepon"
        android:layout_width="286dp"
        android:layout_height="45dp"
        android:layout_alignStart="@+id/rectangle_1"
        android:layout_alignTop="@+id/rectangle_1"
        android:layout_alignEnd="@+id/rectangle_1"
        android:layout_alignBottom="@+id/rectangle_1"
        android:layout_marginStart="33dp"
        android:layout_marginTop="89dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="26dp"
        android:background="@drawable/form_daftar"
        android:ems="10"
        android:drawablePadding="15dp"
        android:drawableLeft="@drawable/search"
        android:fontFamily="@font/poppins"
        android:hint="Pencarian"
        android:inputType="text"
        android:padding="5dp"
        android:paddingStart="15dp"
        android:textColor="@color/black"
        android:textColorHint="#A3A3AE"
        android:textSize="14sp" />

    <ImageView
        android:id="@+id/btnTambah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="72dp"
        android:src="@drawable/tambah_1" />

</RelativeLayout>